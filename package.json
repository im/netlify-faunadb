{
    "name": "netlify-faunadb",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "netlify:server": "netlify-lambda serve functions -c ./webpack.config.js",
        "netlify:build": "netlify-lambda build functions -c ./webpack.config.js",
        "app:serve": "vue-cli-service serve",
        "app:build": "vue-cli-service build",
        "start": "npm-run-all --parallel app:serve netlify:server",
        "build": "npm-run-all --parallel app:build netlify:build"
    },
    "dependencies": {
        "ant-design-vue": "^1.3.15",
        "axios": "^0.19.0",
        "chalk": "^2.4.2",
        "core-js": "^2.6.5",
        "moment": "^2.24.0",
        "vue": "^2.6.10",
        "vue-class-component": "^7.0.2",
        "vue-property-decorator": "^8.1.0",
        "vue-router": "^3.0.3",
        "vuex": "^3.0.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.10.0",
        "@vue/cli-plugin-typescript": "^3.10.0",
        "@vue/cli-service": "^3.10.0",
        "netlify-lambda": "^1.6.3",
        "npm-run-all": "^4.1.5",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.2",
        "typescript": "^3.4.3",
        "vue-template-compiler": "^2.6.10",
        "webpack": "^4.39.3"
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}
